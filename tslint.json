{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "object-literal-shorthand": false,
    "no-console": false,
    "no-trailing-whitespace": false
  },
  "rulesDirectory": []
}
